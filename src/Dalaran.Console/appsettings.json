{
  "serilog": {
    "minimumLevel": {
      "default": "Debug"
    },
    "writeTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "RollingFile",
              "Args": {
                "pathFormat": "logs/dalaran/dalaran-developer-{Date}.log"
              }
            }
          ]
        }
      }
    ]
  }
}